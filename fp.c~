#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>


int do_stuff(){
  return 0;
}

int main(){
  printf("I'm the daddy, my PID is %d\n", getpid());
  int f = fork();
  if (!f){
    
  }
  else{
    int status;
    int cpid = wait(&status);
    printf("The child of pid %d was asleep for %d seconds\n", cpid, WEXITSTATUS(status));
    printf("Daddy is gone.\n");
  }
  return 0;
}
